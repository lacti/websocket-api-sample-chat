{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "await-promise": true
  },
  "linterOptions": {
    "exclude": [
      "**/node_modules/**",
      "**/lib/**",
      ".webpack/**/*.js",
      "webpack.config.js"
    ]
  }
}
